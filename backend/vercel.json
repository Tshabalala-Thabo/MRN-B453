{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Access-Control-Allow-Credentials": "true",
        "Access-Control-Allow-Origin": "https://mrn-b453f.vercel.app",
        "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS",
        "Access-Control-Allow-Headers": "X-CSRF-Token,Authorization,Content-Type,Accept,Origin,X-Requested-With",
        "Access-Control-Expose-Headers": "set-cookie"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}